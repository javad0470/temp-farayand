    <!-- Gateway Overlays -->
    <Style x:Key="GatewayStyle"
           TargetType="ContentControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ContentControl">
                    <Canvas>
                        <Path StrokeThickness="3"
                              Fill="{DynamicResource GateWayBackgroundBrush}"
                              Stroke="{DynamicResource GateWayBorderBrush}">
                            <Path.Effect>
                                <DropShadowEffect />
                            </Path.Effect>
                            <Path.Data>
                                <RectangleGeometry Rect="8, 8, 35, 35">
                                    <RectangleGeometry.Transform>
                                        <RotateTransform Angle="45"
                                                         CenterX="25"
                                                         CenterY="25" />
                                    </RectangleGeometry.Transform>
                                </RectangleGeometry>
                            </Path.Data>
                        </Path>
                        <ContentPresenter HorizontalAlignment="Center"
                                          VerticalAlignment="Center" />
                    </Canvas>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

