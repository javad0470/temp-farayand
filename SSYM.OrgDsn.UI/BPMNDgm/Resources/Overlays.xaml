    <!-- Activity Overlays -->

    <Style x:Key="ActivityStyle"
           TargetType="ContentControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ContentControl">
                    <Border BorderThickness="2"
                            BorderBrush="{DynamicResource ActBorderBrush}"
                            CornerRadius="5"
                            Background="{DynamicResource ActBackgroundBrush}"
                            Padding="2,2,2,3">
                        <Border.Effect>
                            <DropShadowEffect />
                        </Border.Effect>
                        <Grid>

                            <Grid.RowDefinitions>

                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>

                            </Grid.RowDefinitions>

                            <ContentPresenter Grid.Row="1"
                                              HorizontalAlignment="Center"
                                              VerticalAlignment="Bottom" />

                        </Grid>

                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="CallActivityStyle"
           TargetType="ContentControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ContentControl">
                    <Border BorderThickness="5"
                            BorderBrush="{DynamicResource CallActivityBorderBrush}"
                            CornerRadius="5"
                            Background="{DynamicResource CallActivityBackgroundBrush}"
                            Padding="2,2,2,3">
                        <Border.Effect>
                            <DropShadowEffect />
                        </Border.Effect>
                        <Grid>

                            <Grid.RowDefinitions>

                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>

                            </Grid.RowDefinitions>

                            <ContentPresenter Grid.Row="1"
                                              HorizontalAlignment="Center"
                                              VerticalAlignment="Bottom" />

                        </Grid>

                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

