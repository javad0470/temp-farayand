    <Style x:Key="HiveButton"
           TargetType="Button">
        <Setter Property="Cursor"
                Value="Hand" />
        <Setter Property="Width"
                Value="120" />
        <Setter Property="Height"
                Value="120" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Grid>
                        <ed:RegularPolygon x:Name="polygon"
                                           Height="120"
                                           InnerRadius="1"
                                           Margin="0"
                                           PointCount="6"
                                           Stretch="Fill"
                                           UseLayoutRounding="False"
                                           Width="100"
                                           RenderTransformOrigin="0.5,0.5"
                                           Stroke="{DynamicResource appClr1}"
                                           Fill="{DynamicResource appClr1}"
                                           StrokeThickness="2">
                            <ed:RegularPolygon.RenderTransform>
                                <RotateTransform Angle="90" />
                            </ed:RegularPolygon.RenderTransform>
                        </ed:RegularPolygon>
                        <ed:RegularPolygon x:Name="polygon_Highlight"
                                           Height="120"
                                           InnerRadius="1"
                                           Margin="0"
                                           PointCount="6"
                                           Stretch="Fill"
                                           UseLayoutRounding="False"
                                           Width="100"
                                           RenderTransformOrigin="0.5,0.5"
                                           Stroke="{DynamicResource appClr2}"
                                           Fill="{x:Null}"
                                           StrokeThickness="2"
                                           Opacity="0">
                            <ed:RegularPolygon.RenderTransform>
                                <RotateTransform Angle="90" />
                            </ed:RegularPolygon.RenderTransform>
                        </ed:RegularPolygon>
                        <ContentControl Style="{StaticResource OrgImage}"
                                        Margin="20"
                                        Width="Auto"
                                        Height="Auto"
                                        Foreground="White"
                                        Background="{TemplateBinding Background}"></ContentControl>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver"
                                 Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="polygon_Highlight"
                                                         Storyboard.TargetProperty="Opacity"
                                                         To="1"
                                                         Duration="0:0:0.2" />
                                        <!--<ColorAnimation Duration="0:0:0.2"
                                                        To="{StaticResource appClr2_c}"
                                                        Storyboard.TargetProperty="(Shape.Stroke).(SolidColorBrush.Color)"
                                                        Storyboard.TargetName="regularPolygon" />-->
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Storyboard.TargetName="polygon_Highlight"
                                                         Storyboard.TargetProperty="Opacity"
                                                         To="0"
                                                         Duration="0:0:0.2" />
                                        <!--<ColorAnimation Duration="0:0:0.2"
                                                        To="{StaticResource appClr1_c}"
                                                        Storyboard.TargetProperty="(Shape.Stroke).(SolidColorBrush.Color)"
                                                        Storyboard.TargetName="regularPolygon" />-->
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

