    <SolidColorBrush  x:Key="MenuStatusBar_Error">#FFCD382D</SolidColorBrush >


    <LinearGradientBrush x:Key="MenuBackground"
                         EndPoint="0.5,1"
                         StartPoint="0.5,0">
        <GradientStop Color="#FF41B1E1" />
        <GradientStop Color="#FFBDBDBD"
                      Offset="0.685" />
        <GradientStop Color="#FF8DB8CB"
                      Offset="0.341" />
    </LinearGradientBrush>

    <!--ToolTip-->
    <SolidColorBrush x:Key="ToolTipForSlcObjRstBackground"
