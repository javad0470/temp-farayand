    <Color x:Key="appClr1_c">#FF1F2737</Color>

    <Color x:Key="appClr1Opac_c">#AA1F2737</Color>

    <Color x:Key="appClr2Opac_c">#AAC8C800</Color>

    <Color x:Key="appClr2_c">#FFC8C800</Color>

    <Color x:Key="fontClr_c">White</Color>

    <!--#FFEFEFEF-->
    <Color x:Key="alternateRow_c">Silver</Color>

    <Color x:Key="appBG_c">#FFFFFFFF</Color>

    <SolidColorBrush  x:Key="appClr1Opac"
